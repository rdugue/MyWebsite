{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@types/nuxtjs__auth" /*"@nuxtjs/auth"*/,
      "@types/marked",
      "@types/lodash",
      "buefy/types",
      "@nuxtjs/axios",
      "@types/bcrypt"
    ]
  },
  "exclude": ["node_modules"]
}
